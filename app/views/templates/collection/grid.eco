<h1>
  <%= @channel.title %>
</h1>

<div class="gallery">
  <% for block in @blocks: %>
    <% if block.image_thumb: %>
      <div class="thumb">
        <div class="image">
          <a href="/#/<%= @channel.slug %>/view:<%= block.id %>">
            <img src="<%= block.image_thumb %>" alt="<%= block.title %>" />
          </a>
        </div>
      </div>
    <% end %>
  <% end %>
</div>