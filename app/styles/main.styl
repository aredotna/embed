@import 'nib'

body
  font-size: 12px
  line-height: 18px;
  color: #111
  background: #fff
  font-family: sans-serif
  &.loading
    background: #fff url('../images/load.gif') top left no-repeat

ul
  list-style-type: none

a, a:visited
  color: #F95A5A
  text-decoration: none

a:hover
  color: #665248

#content
  margin: 5%
